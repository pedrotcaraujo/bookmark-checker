{
  "manifest_version": 2,

  "name": "Bookmark Checker",
  "description": "Check if old favorites URL is valid",
  "version": "1.0",
  "browser_action": {
    "default_title": "Bookmark Checker",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
